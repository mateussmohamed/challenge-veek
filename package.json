{
  "name": "veek",
  "version": "0.0.0",
  "description": "veek",
  "scripts": {
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "next lint",
    "lint:css": "stylelint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:all": "yarn run lint && yarn run lint:css",
